.title {
  background-color: orange;
  height:90rpx;
  width: 100%;

}

.club_message {
  font-size: 30rpx;
  flex:3;
  text-align:center;
  line-height: 90rpx;
  color:white;

}

.next {
  text-align: center;
  line-height: 90rpx;
  border: 3rpx solid orange;
  border-radius: 10rpx;
  width: 150rpx;
  height: 50rpx;
  background: orange;
  margin-left: 300rpx;
  margin-top:50rpx;
}
.nextbutton{
  font-size: 26rpx;
  color: white;
  background: orange;
  line-height: 50rpx;
}
.return {
    padding-bottom: 20rpx;
    width: 60rpx;
    margin-top:10rpx;
    flex: 1;

}

.tab_top {
  display: -webkit-flex;
   direction: column;

}


.head{
    padding-left: 270rpx;
    padding-top:100rpx;
}
.border{
    width: 200rpx;
    height:200rpx;
    text-align: center;
    border-radius: 50%;
    color: orange;
    border: 5rpx solid orange;
}
.border text{
    line-height: 200rpx;
    font-size: 200rpx;
}
.upload{
    text-align: center;
    font-size: 24rpx;
    padding-top:40rpx;
}




.add {
  height: 250rpx;
  width:250rpx;
}

.image {
  width: 100%;
  text-align: center;
  margin-top: 30rpx;
}

.input_border {
  border-bottom: 2rpx solid #ffbf2c;
  margin-left: 150rpx;
  width: 60%;
}

.input {
  text-align: center;
  margin-top: 60rpx;
}

.string {
  text-align: center;
}

.choose {
  text-align: center;
  color: #ffbf2c;
  margin-top: 100rpx;
  font-size: 26rpx;
}

.click {
  display: flex;
  justify-content: center;
  text-align: center;
  margin-top: 200rpx;
}

.click_next {
  font-size: 26rpx;
  color: gray;

  display: flex;
  justify-content: center;
}

.user {
  font-size: 26rpx;
  color: #ffbf2c;
  border-bottom: 2rpx solid #ffbf2c;
}

.choose_type {
  /*浮层代码*/
  background-color: rgba(0, 0, 0, 0.5);
  height: 1400rpx;
  width: 100%;
  position: fixed;
  z-index: 999;
  top: 0;
}

.column{
    width: 10rpx;
    height: 50rpx;
    padding-top: 20rpx;
    padding-left: 20rpx;
}
.choice{
    height: 120rpx;
    display:-webkit-flex;
}
.choice view{
    width: 140rpx;
    height: 60rpx;
    text-align: center;
    line-height: 70rpx;
    font-size: 30rpx;
    margin-top: 20rpx;
    margin-left: 10rpx;
}
.one{
    flex: 1;
}
.style{
    color: orange;
    border-radius: 40rpx;
    border: 3rpx solid orange;
}
.stylex{
    color: white;
    background: orange;
    border-radius: 40rpx;
    border: 3rpx solid orange;
}
.confirm{
    width: 180rpx;
    padding-left: 380rpx;
    padding-top:100rpx;
    padding-bottom:50rpx;
    display:-webkit-flex;
}
.no{
    flex: 1;
    font-size: 28rpx;
}
.ok{
    flex: 1;
    color: orange;
    font-size: 28rpx;
}
.foot{
    padding-top:500rpx;
    display:-webkit-flex;
}
.front{
    flex: 2;
    text-align: right;
    font-size: 24rpx;
}
.after{
    flex: 1;
    color: orange;
    text-align: left;
    font-size: 24rpx;
}





.choose_detail {
  background-color: white;
  width: 75%;
  height: 40%;
  margin-left: 13%;
  margin-top: 40%;
}

.choose_club {
  margin-top: 20rpx;

}

.var_org {
  display: inline-flex;
  width: 100%;
  margin-top: 20rpx;
}

.tag_top {
  font-size: 26rpx;
  border: 2rpx solid #ffbf2c;
  border-radius: 15%;
  margin-left: 5%;
  margin-right: 5%;
  width: 200rpx;
  color: #ffbf2c;
}

.tag_mid {
  font-size: 26rpx;
  border: 2rpx solid #ffbf2c;
  border-radius: 15%;
  margin-left: 13%;
  margin-right: 13%;
  width: 110rpx;
  color: #ffbf2c;
}

.cancel_sure {
  display: inline-flex;
  
  width: 100%;
  margin-top: 20rpx;
  margin-bottom: 20rpx;
}

.cancel {
  margin-left: 65%;
  font-size: 26rpx;
  margin-right: 10%;
  width: 60rpx;
  color: #ffbf2c;
  
}

.sure {
  font-size: 26rpx;
  color: #ffbf2c;
  
}
