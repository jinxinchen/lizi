  <input bindinput="inputintro" value="{{intro}}" />
  <view>社团所在</view>
  <input bindinput="inputschool" value="{{school_id}}" />
  <view>社团所属</view>
  <input bindinput="inputtag" value="{{tag_id}}" />
</view>
<button bindtap="Set">保存</button>

</view>